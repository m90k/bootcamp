{
  "name": "gomeetapp-backend",
  "version": "1.0.0",
  "description": "Projeto do Backend da Aplicacao MeetApp que tera versoes web e mobile feitas em react e react-native",
  "main": "index.js",
  "author": "Marcos de Souza",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/Server.js",
    "debugDev": "node --inspect-brk -r sucrase/register src/server.js",
    "queue": "nodemon src/queue.js",
    "debugQueue": "node --inspect-brk -r sucrase/register src/queue.js",
    "build": "sucrase ./src -d ./dist --transforms imports",
    "start-app": "node dist/server.js",
    "start-queue": "node dist/queue.js"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "sucrase": "^3.12.0"
  },
  "dependencies": {
    "@sentry/node": "^5.10.2",
    "bcryptjs": "^2.4.3",
    "bee-queue": "^1.2.2",
    "cors": "^2.8.5",
    "date-fns": "^2.8.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-error": "^0.0.2",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.2",
    "nodemailer-express-handlebars": "^3.1.0",
    "sequelize": "^5.21.3",
    "youch": "^2.0.10",
    "yup": "^0.28.0"
  }
}
